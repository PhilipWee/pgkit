{
  "name": "pgkit",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Bundled package of all the pgkit libraries",
  "files": [
    "dist"
  ],
  "exports": {
    "client": {
      "import": "./dist/client.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "bin": {
    "pgkit": "./dist/cli.js"
  },
  "author": "mmkal",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmkal/pgkit",
    "directory": "packages/pgkit"
  },
  "scripts": {
    "dogfood": "tsx src/cli.ts",
    "prepack": "pnpm build",
    "build": "tsc --sourceMap -p tsconfig.lib.json",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.20",
    "eslint": "^8.57.0",
    "eslint-plugin-mmkal": "0.10.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "@pgkit/admin": "workspace:^",
    "@pgkit/client": "workspace:^",
    "@pgkit/migrator": "workspace:^",
    "@pgkit/typegen": "workspace:^",
    "express": "^4.18.2",
    "importx": "^0.4.4",
    "trpc-cli": "https://pkg.pr.new/mmkal/trpc-cli@38"
  }
}
